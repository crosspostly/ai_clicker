{
  "name": "ai-autoclicker",
  "version": "2.0.0",
  "description": "AI-powered browser extension for web automation using Google Gemini.",
  "main": "build.js",
  "scripts": {
    "build": "node build.js",
    "build:prod": "NODE_ENV=production node build.js",
    "build:watch": "nodemon --watch src build.js",
    "clean": "rm -rf deploy *.zip",
    "lint": "eslint src/ --fix",
    "test": "jest",
    "format": "prettier --write 'src/**/*.js'"
  },
  "keywords": ["chrome","extension","automation","AI","gemini"],
  "author": "crosspostly",
  "license": "MIT",
  "devDependencies": {
    "fs-extra": "^11.2.0",
    "nodemon": "^3.0.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0"
  }
}
