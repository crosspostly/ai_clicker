{
  "name": "ai-autoclicker",
  "version": "2.0.0",
  "description": "AI-powered browser extension for web automation using Google Gemini.",
  "main": "build.js",
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "build:prod": "NODE_ENV=production node build.js",
    "build:watch": "nodemon --watch src build.js",
    "clean": "rm -rf deploy *.zip",
    "lint": "eslint src/ --fix",
    "test": "jest",
    "format": "prettier --write 'src/**/*.js'"
  },
  "keywords": [
    "chrome",
    "extension",
    "automation",
    "AI",
    "gemini"
  ],
  "author": "crosspostly",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^8.54.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.1.0",
    "rollup": "^4.53.1"
  }
}
